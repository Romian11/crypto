<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <link rel="stylesheet" href="/navbar/css/style.css">
     <link rel="stylesheet" href="/Dashboard/css/style.css">
     <link rel="stylesheet" href="/addbtn/css/style.css">
     {{>script}}
    <title>Document</title>
</head>
  <style>
    
   .box { height: 450px !important;
    } #submit_btn { margin-top: 30px !important; }
    input[type="number"]::-webkit-inner-spin-button,
    input[type="number"]::-webkit-outer-spin-button { -webkit-appearance: none;
    margin: 0; }
  </style>

  <script>
    // Get the number input element 
    const numberInput =document.getElementById("myNumberInput"); // Disable the increment and
    numberInput.addEventListener("focus", function () {
    this.addEventListener("keydown", function (event) { if (event.which === 38
    || event.which === 40) { event.preventDefault(); } }); });
  </script>
<body>
<div class="main">
    <div class="top">
        {{>navbar}}
    </div>
    <div class="bottom">
       <div class="head">
        <p class="selected">Dashboard<i class="fa fa-credit-card" aria-hidden="true"></i></p>
        <div class="separator"></div>
       </div>
       <div class="user_wallet">
        <div class="info">
        <div class="currbalance">
            <span>Name</span>
       <span id="name">-</span></div>
        <div class="accounts">
          
            <div class="invested">
              <span>E-mail</span> 
             <span><span id="E_mail">-</span></span>
              </div>

         <div class="starting_balance">
          <span>Contact Number</span>
             <span><span id="P_no">-</span></span>
          </div>
        <div class="user_age">
            <span>Age</span>
             <span><span id="age">-</span></span>
          </div>

        </div>
        <div class="profit_loss">
            <div class="starting_balance">
              <span>Available Balance</span> 
              <span><span id="profit_loss_">$</span><span id="balance">0.00</span></span>
              </div>
        </div>
        </div>
        <div class="deposit_money">
          <div class="money_input">
            <div class="deposit">Your Portfolio</div>
          
           
        </div>
       </div>
    </div>
</div>
<script  src="/dashboard/js/script.js"></script>
<script  src="/dashboard/js/script1.js" type="module"></script>
</body>
</html>